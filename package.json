{
  "name": "yukiswapi",
  "version": "0.0.1",
  "description": "Yuki Swapi Project",
  "productName": "YukiSwapi",
  "author": "Johnny Driesen <info@3sn.be>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "e2e": "start-server-and-test dev http://localhost:9000 cy:run",
    "e2e:open": "start-server-and-test dev http://localhost:9000 cy:open",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^6.0.3",
    "@cypress/vue": "^6.0.2",
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@pinia/testing": "^1.0.2",
    "@quasar/app-vite": "^2.1.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.5.9",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.2",
    "cypress": "^14.5.3",
    "eslint": "^9.14.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.13",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.9.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
